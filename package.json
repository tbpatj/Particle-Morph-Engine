{
  "name": "particle-morph",
  "version": "1.0.2",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "parcel src/index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && parcel build lib/index.js --dist-dir lib --target default",
    "build_2": "parcel build src/index.ts --public-url ./ --no-source-maps --dist-dir ./build --no-cache",
    "build_serve": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tbpatj/Particle-Morph-Engine.git"
  },
  "keywords": [
    "particles",
    "morph",
    "engine",
    "webgl",
    "particle-swarm-optimization",
    "particle-system",
    "interactive-visualizations"
  ],
  "author": "TJ Luebke <luebkedev@gmail.com>",
  "license": "MIT",
  "homepage": "https://tbpatj.github.io/Particle-Morph-Engine/demo/",
  "devDependencies": {
    "parcel": "^2.12.0",
    "typescript": "^5.5.2"
  },
  "targets": {
    "default": {
      "source": "lib/index.js",
      "distDir": "lib",
      "publicUrl": "./",
      "engines": {
        "browsers": "> 0.25%, not dead"
      }
    }
  },
  "files": [
    "lib/**/*",
    "src/**/*",
    "README.md",
    "LICENSE"
  ]
}
